---
import { ICON } from './icon'
import IconAngular from './iconAngular.astro'
import IconAstro from './iconAstro.astro'
import IconDatabase from './iconDatabase.astro'
import IconNet from './iconNet.astro'
import IconReact from './iconReact.astro'
import IconTailwind from './iconTailwind.astro'
import IconTypescript from './iconTypescript.astro'

interface Props {
	type: ICON
}

const { type } = Astro.props
---

{
	{
		[ICON.ANGULAR]: <IconAngular />,
		[ICON.REACT]: <IconReact />,
		[ICON.TYPESCRIPT]: <IconTypescript />,
		[ICON.TAILWIND]: <IconTailwind />,
		[ICON.DATABASE]: <IconDatabase />,
		[ICON.DOTNET]: <IconNet />,
		[ICON.ASTRO]: <IconAstro />
	}[type]
}
